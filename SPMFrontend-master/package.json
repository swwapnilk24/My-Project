  "main": "bundle.js",
  "config": {
    "app": {
      "name": "spm"
    },
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build:dev": "webpack --env dev -p",
    "build:prod": "webpack --env prod -p",
    "build:preprod": "webpack --env preprod -p",
    "build:qa": "webpack --env qa -p",
    "build:int": "webpack --env int -p",
    "startdev": "webpack-dev-server --host 127.0.0.1 --port 8080 --env development --hot",
    "test": "jest --config jest.json",
    "postedinstall": "webpack -p",
    "start": "node server.js",
    "test:coverage": "npm test -- --coverage",
    "test:watch": "npm test -- --watch",
    "commit": "git-cz",
    "release": "standard-version --no-verify",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "pssrecommit": "npm run lint",
    "commitmsg": "validate-commit-msg",
    "lint": "eslint src"
